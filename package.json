{
  "name": "cs.js",
  "version": "0.0.2",
  "description": "Algorithms and data structures in JavaScript",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run clean && ./node_modules/.bin/babel ./src --out-dir lib",
    "build:watch": "./node_modules/.bin/babel ./src --watch --out-dir lib",
    "clean": "rm -rf lib",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "./node_modules/.bin/eslint --fix ./src",
    "test": "./node_modules/.bin/jest",
    "test:unit": "./node_modules/.bin/jest",
    "test:unit:watch": "./node_modules/.bin/jest --watchAll",
    "test:unit:lib": "./node_modules/.bin/jest 'test/lib.test.js'",
    "test:unit:lib:watch": "./node_modules/.bin/jest 'test/lib.test.js' --watchAll"
  },
  "repository": "https://github.com/jesselpalmer/cs.js",
  "engines": {
    "node": ">=4.2.4"
  },
  "keywords": [
    "Algorithms",
    "CS",
    "CS Algoorithms",
    "Computer Science",
    "Data Structures"
  ],
  "author": "Jesse Palmer <jesselpalmer@gmail.com> (https://jesselpalmer.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/cs.js/issues"
  },
  "homepage": "https://github.com/jesselpalmer/cs.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.9.0",
    "chai": "^4.2.0",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^28.1.0",
    "jsdoc": "^3.6.2"
  },
  "private": false,
  "funding": {
    "type" : "github",
    "url" : "https://github.com/sponsors/jesselpalmer"
  }
}
